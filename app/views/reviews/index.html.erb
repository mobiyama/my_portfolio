<h1 class="text-center">タイトル</h1>
<div class="row">
  <%=link_to "投稿する",new_review_path,class:"btn btn-primary"%>
</div>
<div class="row">
  <% @reviews.each do |review| %>
  <div class="col-md-8 rev-card">
    <%= link_to review_path(review) do %>
    <p class="rev-card-title"><%= review.title %></p>
    <p><%= review.created_at.strftime("%Y年%m月%d日") %></p>
    <%= image_tag "food_sample.jpeg", class: "img-fluid" %>
    <% end %>
  </div>
  <% end %>
</div>
